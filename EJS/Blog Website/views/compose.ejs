<%- include("partials/header");  -%>

<h1>Candidate's Details</h1>

<form class="" action="/compose" method="post">

    <div class="form-group">

        <br>
        <br>
    <label>Name</label>
    <input class="form-control" type="text" name="postTitle">
    <label>Phone Number</label>
    <input class="form-control" type="text" name="postNumber">
    <label>E-Mail</label>
    <input class="form-control" type="text" name="postMail">
    <label>Qualification</label>
    <input class="form-control" type="text" name="postQualification">
    <label>Age</label>
    <input class="form-control" type="text" name="postAge">
    <br>
    
    <label>Feedback-</label>
    <textarea class="form-control" name="postBody" cols="30" rows="5"></textarea>
    
</div>

<button class="btn btn-primary" type="submit" name="button">Publish</button>
</form>

<%- include("partials/footer");  -%>